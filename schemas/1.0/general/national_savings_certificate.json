{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "ministryofjustice/laa-criminal-legal-aid-schemas/main/schemas/1.0/general/national_savings_certificate.json",
  "title": "National Savings Certificate",
  "description": "Holds attributes of an National Savings Certificate",
  "type": "object",
  "properties": {
    "holder_number": { "type": "string" },
    "certificate_number": { "type": "string" },
    "value": { "type": "integer" },
    "ownership_type": { "type": ["string"], "enum": ["applicant", "partner"] }
  },
  "required": ["value", "certificate_number", "holder_number", "ownership_type"]
}
