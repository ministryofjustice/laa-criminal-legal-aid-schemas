  "$id": "ministryofjustice/laa-criminal-legal-aid-schemas/main/schemas/1.0/general/provider.json",
  "title": "Provider",
  "description": "Holds common attributes of a provider",
  "type": "object",
  "properties": {
    "office_code": { "type": "string" },
    "provider_email": { "type": "string" },
