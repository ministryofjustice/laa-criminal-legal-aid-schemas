{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "ministryofjustice/laa-criminal-legal-aid-schemas/main/schemas/1.0/general/ioj.json",
  "title": "Ioj",
  "description": "Holds common attributes of an interest of justice",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "loss_of_liberty", "suspended_sentence", "loss_of_livelihood", "reputation", "question_of_law",
        "understanding", "witness_tracing", "expert_examination", "interest_of_another", "other"
      ]
    },
    "reason": { "type": "string" }
  },
  "required": ["type", "reason"]
}
